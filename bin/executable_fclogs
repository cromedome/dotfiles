#!/bin/bash

CONTAINER=$1

if [[ -z $CONTAINER ]]; then
    echo 'container is required'
    return 1
fi

cd ~/code/csuitetools/docker/compose/dev
#docker-compose logs -f --tail=20 $CONTAINER
#docker-compose logs -f --tail=20 $CONTAINER | cut -d'|' -f2- | jq -R 'fromjson? | select(.host != "content-local.fcsuite.com")'
docker-compose logs -f --tail=20 $CONTAINER | cut -d'|' -f2- | jq -R 'fromjson? | select(.log_type == "csuite")'
cd -

